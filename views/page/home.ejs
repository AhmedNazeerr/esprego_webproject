<!-- facility section ends -->

<!-- menu section starts  -->

<section class="menu" id="menu">

   <div class="heading">
      <img src="images/heading-img.png" alt="" >
      <h3>popular menu</h3>
   </div>

   <div class="box-container">

      <div class="box">
        <a href="/product">
