      <img src="images/heading-img.png" alt="">
      <h3>contact us</h3>
   </div>

   <div class="row">

      <div class="image">
         <img src="images/contact-img.svg" alt="">
      </div>

      <form action="/contactus" method="post">
         <h3>Spill the beans, and we'll get back to you a little latte-r.</h3>
         <input type="text" name="contactname" required class="box" maxlength="50" placeholder="enter your name">
         <input type="email" name="contactemail"  required class="box" maxlength="70" placeholder="enter your email"  >
         <input type="text" name="contactmessage" required  class="box" maxlength="200" placeholder="enter your Message" >
         <input type="submit"  value="send message" class="btn">
      </form>

   </div>

</section>

<!-- contact section ends -->
<section>


   <div class="faq_area section_padding_130" id="faq" style="    margin-top: -150px;">
      <div class="container">
          <div class="row justify-content-center">
              <div class="col-12 col-sm-8 col-lg-6">
   
                  <div class="heading">
                     <img src="images/heading-img.png" alt="">
                     <%if(faq.length>0){%>
                     <h3>Frequently Asked Questions</h3>
                     <%}else{%>
                        <h3>No Faqs are Avaliable Right Now</h3>
                        <%}%>
                  </div>
              </div>
          </div>
          <%
          if(faq.length>0){
             faq.forEach(element => {
          %>
          <div class="row justify-content-center" style="color:black;">
    
              <div class="col-12 col-sm-10 col-lg-8" style="margin: 23px;" >
                  <div class="accordion faq-accordian" id="faqAccordion" style="box-shadow: 500px black; border: 2px solid rgb(198, 198, 198);">
                      <div class="card border-0 wow fadeInUp" data-wow-delay="0.2s" style="visibility: visible; animation-delay: 0.2s; animation-name: fadeInUp;">
                          <div class="card-header" id="heading<%=element.id%>" >
                              <h6 class="mb-0 collapsed" data-toggle="collapse" data-target="#collapse<%=element.id%>" aria-expanded="true" aria-controls="collapse<%=element.id%>"style="font-size: 2.5rem;color:black;"><%=element.q%>?<span class="lni-chevron-up" ></span></h6>
                          </div>
                          <div class="collapse" id="collapse<%=element.id%>" aria-labelledby="heading<%=element.id%>" data-parent="#faqAccordion">
                              <div class="card-body">
                                  <p style="font-size:1.5rem ;"><%=element.a%></p>
                              </div>
                          </div>
                  </div>
              </div>
          </div>
      </div>
<%
   })
          } %>
  </div>


</section>


