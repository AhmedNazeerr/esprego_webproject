      <div class="alert alert-warning alert-dismissible fade show" style="font-size: 20px;" role="alert">
        <%=message%>
        <button type="button" class="close" data-dismiss="alert" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <%}%>
</header>

<!-- header section ends -->

<!-- home section starts  -->

